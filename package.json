{
  "name": "s3-upload",
  "version": "1.0.0",
  "description": "GitHub action to upload files to Amazon S3",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.json",
    "release": "ncc build src/s3-upload.ts -o action && git add -f action/",
    "clean": "rimraf -- action",
    "lint": "eslint **/*.ts"
  },
  "keywords": [
    "Actions",
    "GitHub",
    "AWS",
    "S3",
    "Upload"
  ],
  "author": {
    "name": "Giao Ho",
    "email": "joutvhu@gmail.com",
    "url": "https://github.com/joutvhu"
  },
  "license": "MIT",
  "homepage": "https://github.com/joutvhu/s3-upload#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joutvhu/s3-upload.git"
  },
  "bugs": {
    "url": "https://github.com/joutvhu/s3-upload/issues"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "aws-sdk": "^2.1529.0",
    "mime-types": "^2.1.35",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
